{
  "name": "vue-konva",
  "version": "2.0.0",
  "description": "Vue binding to canvas element via Konva framework",
  "keywords": [
    "vue",
    "konva",
    "canvas"
  ],
  "homepage": "https://github.com/konvajs/vue-konva#readme",
  "author": "Rafael Escala <rafaesc92@gmail.com>",
  "main": "lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konvajs/vue-konva.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konvajs/vue-konva/issues"
  },
  "scripts": {
    "build": "npm run build:dist && npm run build:lib",
    "build:dist": "webpack",
    "build:lib": "babel src --presets=@babel/preset-env --out-dir lib",
    "clean": "rm -rf dist lib",
    "test": "karma start --single-run",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "peerDependencies": {
    "konva": "^2.0.0",
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.28",
    "chai": "^4.2.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "konva": "^2.0.0",
    "mocha": "^5.2.0",
    "puppeteer": "^1.11.0",
    "sinon": "^6.3.5",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
